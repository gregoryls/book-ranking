{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "no-param-reassign": ["error", { "props": false }],
    "import/extenstions": [
      "error",
      "ignorePackages",
      {
        "js": "always",
        "json": "always"
      }
    ]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extenstions": [".js", ".json"]
      }
    }
  }
}
